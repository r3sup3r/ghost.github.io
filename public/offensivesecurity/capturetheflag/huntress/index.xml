    <item>
      <title>Huntress CTF 2024</title>
      <link>http://localhost:1313/offensivesecurity/capturetheflag/huntress/huntress2024/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
